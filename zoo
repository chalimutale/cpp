// 1. Определить, что при делении числа а на число b получается остаток, равный одному из заданных чисел c или d
#include <stdio.h>
/* Определить, что при делении числа а на число b получается  остаток, 
               равный одному из заданных чисел с или d*/
int main() {
    int a,b,c,d;
    printf("Програма определит, что при делении числа а на число b получается остаток, равный одному из заданных чисел с или d\n");
    printf("Введение a и b");
    scanf("%d %d", &a, &b);
    printf("Введение c и d ");
    scanf("%d %d", &c, &d);
    if(a % b == c || a % b == d) 
        printf("Остаток от леления числа %d a %d равен %d или %d", a, b, c, d);
        
    else
        printf("Остаток от леления числа %d a %d не равен %d  и %d", a, b, c, d);
    }
